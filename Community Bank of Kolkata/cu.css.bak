.btn-group button {
  background-color: #04AA6D; 
  border: 1px solid green; 
  color: white; 
  padding:10px 24px;	
  cursor: pointer; 
  float: left; 
}

.btn-group:after {
  content: "";
  clear: both;
  display: table;
}

.btn-group button:not(:last-child) {
  border-right: none; 
}

.btn-group button:hover {
  color:white;
  background-color:red;
